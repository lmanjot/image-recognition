{
  "version": 2,
  "functions": {
    "api/upload.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/camera.html",
      "dest": "/camera.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}